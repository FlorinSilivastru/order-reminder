{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Ocelot": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "customerRoute": {
        "ClusterId": "customerCluster",
        "AuthorizationPolicy": "CookieAuthenticationPolicy",
        "Match": {
          "Path": "/api/v1/customer/{**catch-all}"
        }
      },
      "businessRoute": {
        "ClusterId": "businessCluster",
        "AuthorizationPolicy": "CookieAuthenticationPolicy",
        "Match": {
          "Path": "/api/v1/business/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "customerCluster": {
        "Destinations": {
          "customerRoute": {
            "Address": "https://localhost:7254"
          }
        }
      },
      "businessCluster": {
        "Destinations": {
          "businessRoute": {
            "Address": "https://localhost:7031"
          }
        }
      }
    }
  }
}
